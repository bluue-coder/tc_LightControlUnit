{
  "$schema": "http://json.schemastore.org/tsconfig",
  "compileOnSave": true,
  "compilerOptions": {
    "module": "none",
    "target": "es6",
    "skipLibCheck": true,
    "lib": [
      "dom",
      "scripthost",
      "es6",
      "es2017.intl"
    ],
    "types": [],
    "declaration": true,
    "sourceMap": true,
    "noEmitOnError": true,
    "suppressImplicitAnyIndexErrors": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictBindCallApply": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "strictFunctionTypes": false,
    "strictPropertyInitialization": false,
    "alwaysStrict": false
  },
  "include": [
    "$(Beckhoff.TwinCAT.HMI.Framework).InstallPath/TcHmi.d.ts",
    "$(Beckhoff.TwinCAT.HMI.Controls).InstallPath/TcHmiEllipse/TcHmiEllipse.d.ts",
    "$(Beckhoff.TwinCAT.HMI.Controls).InstallPath/TcHmiTextblock/TcHmiTextblock.d.ts",
    "$(Beckhoff.TwinCAT.HMI.Controls).InstallPath/TcHmiTextbox/TcHmiTextbox.d.ts",
    "$(Beckhoff.TwinCAT.HMI.Controls).InstallPath/TcHmiToggleSwitch/TcHmiToggleSwitch.d.ts",
    "$(Beckhoff.TwinCAT.HMI.Controls).InstallPath/TcHmiCheckbox/TcHmiCheckbox.d.ts",
    "$(Beckhoff.TwinCAT.HMI.Controls).InstallPath/TcHmiNumericInput/TcHmiNumericInput.d.ts",
    "$(Beckhoff.TwinCAT.HMI.BA.Framework).InstallPath/index.d.ts"
  ],
  "files": [
    "controlLightMinimal/controlLightMinimal.ts",
    "controlPopUpLightDetails/controlPopUpLightDetails.ts"
  ]
}